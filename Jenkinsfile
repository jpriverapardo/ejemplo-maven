pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Build') {
            steps {
                echo 'Building declarative Pipeline'
            }
        }
        stage('Testing Declarative Pipeline') {
            steps {
                echo 'Testing declarative Pipeline'
            }
        }
        stage('Finish') {
            steps {
                echo 'Finish declarative Pipeline'
            }
        }
    }
}
